  <body>
    <%= yield_content(:head)%>
    

    <div id="labEprouvette"></div>
    <div id="headerArea">
        <header>
            <h1><%= yield_content(:header)%></h1>
        </header>
    </div>
    <div id="wrapper">
        <h2><%= yield_content(:intro)%></h2>
        <nav>
            <%= partial "shared/navlinks" %>
        </nav>
    
        <% content = yield %>
        <%
        content.gsub!("<img", "<amp-img")
        content.gsub!("<iframe", "<amp-iframe")
        content.gsub!("</img", "</amp-img")
        content.gsub!("</iframe", "</amp-iframe")
        %>
